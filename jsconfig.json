{
    "compilerOptions": {
        "module": "ES6",
        "moduleResolution": "node",
        "target": "ES6",
        "allowImportingTsExtensions": true,
        "noImplicitAny": false,
        "checkJs": true
    },
    "typeAcquisition": {
        "enable": false
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}
